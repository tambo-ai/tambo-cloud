{
  "name": "@tambo-ai-cloud/repo",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "lint:fix": "turbo lint -- --fix",
    "test": "turbo test",
    "check-types": "turbo check-types",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "prettier-check": "prettier --check .",
    "hydra-api:start": "turbo run start --filter=hydra-api",
    "prepare": "husky",
    "db:generate": "cd packages/db && npm run db:generate",
    "db:migrate": "cd packages/db && npm run db:migrate",
    "db:check": "cd packages/db && npm run db:check",
    "db:studio": "cd packages/db && npm run db:studio",
    "test:suggestions": "cd scripts && tsx test-suggestions.ts",
    "rename-to-tambo": "ts-node scripts/update-content.ts && ts-node scripts/rename-files.ts",
    "rename-vnext": "ts-node scripts/rename-vnext.ts"
  },
  "devDependencies": {
    "@svgr/webpack": "^8.1.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.0",
    "prettier": "^3.5.3",
    "ts-node": "^10.9.2",
    "turbo": "^2.5.0",
    "typescript": "^5.8.3"
  },
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "engines": {
    "node": ">=20",
    "npm": ">=10"
  },
  "volta": {
    "node": "20.19.0",
    "npm": "10.9.2"
  },
  "packageManager": "npm@10.9.2",
  "version": "0.36.0"
}
