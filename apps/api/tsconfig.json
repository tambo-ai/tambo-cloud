    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "outDir": "./dist",
    "baseUrl": "./",
    "noImplicitAny": false,
    "moduleResolution": "node",
    "module": "commonjs",
    "resolveJsonModule": true,
    "sourceMap": true,
    "paths": {
      "@tambo-ai-cloud/hydra-ai-server": ["../../packages/hydra-ai-server/src"],
      "@tambo-ai-cloud/core": ["../../packages/core/src"],
      "@tambo-ai-cloud/db": ["../../packages/db/src"]
