{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "entryFile": "apps/api/src/main",
  "compilerOptions": {
    "deleteOutDir": true,
    "plugins": ["@nestjs/swagger"]
  },
  "projects": {
    "generate-config": {
      "generateOptions": {},
      "type": "application",
      "root": "apps/api",
      "entryFile": "apps/api/src/generate-config",
      "sourceRoot": "src"
    }
  }
}
