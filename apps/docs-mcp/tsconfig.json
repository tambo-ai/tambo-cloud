{
  "$schema": "https://json.schemastore.org/tsconfig",
  "extends": "../../packages/typescript-config/nextjs.json",
  "compilerOptions": {
    "strict": true,
    "noEmit": true
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "next-env.d.ts",
    "*.mjs",
    ".next/types/**/*.ts",
    "types/*.d.ts"
  ],
  "exclude": ["node_modules", ".next"]
}
