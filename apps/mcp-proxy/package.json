{
  "name": "@tambo-ai-cloud/mcp-proxy",
  "version": "0.0.1",
  "description": "A proxy for Tambo that connects to all known MCP servers for a given project",
  "type": "module",
  "main": "dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc --project tsconfig.json",
    "build:esm": "tsc --project tsconfig.esm.json",
    "start": "node dist/index.js",
    "lint": "eslint",
    "test": "jest --passWithNoTests",
    "check-types": "tsc --noEmit"
  },
  "author": "",
  "license": "Apache-2.0",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.20.1",
    "commander": "^14.0.1",
    "express": "^5.1.0"
  },
  "devDependencies": {
    "@types/node": "^22.15.32",
    "typescript": "^5.9.3"
  }
}
